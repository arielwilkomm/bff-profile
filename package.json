{
  "name": "bff-profile",
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env environment=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx,mdx}\"",
    "lint:report": "eslint --quiet --ext .js,.jsx,.ts,.tsx src -f json -o test/coverage/eslint-report.json .",
    "lint:formater": "yarn format && yarn lint",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --ci --collectCoverage --maxWorkers=40%",
    "test:watch": "jest --ci --collectCoverage --maxWorkers=40%",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:custom": "bash -c 'yarn test > test-results.log 2>&1; if grep -q \"^FAIL\" test-results.log; then grep -A 20 \"^FAIL\" test-results.log | GREP_COLORS=\"mt=1;31\" grep --color=always -E \"FAIL|at Object.<anonymous>\"; else grep -E \"PASS|Test Suites:|Tests:|Snapshots:|Time:|Ran all test suites\" test-results.log | sed -E \"s/(PASS)/\\x1b[32m\\1\\x1b[0m/g; s/(FAIL)/\\x1b[31m\\1\\x1b[0m/g; s/(Test Suites:|Tests:|Snapshots:|Time:|Ran all test suites)/\\x1b[34m\\1\\x1b[0m/g\"; fi'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^10.0.1",
    "@pact-foundation/pact": "^13.1.0",
    "axios": "^1.9.0",
    "axios-mock-adapter": "^1.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.10",
    "eslint-import-resolver-typescript": "^4.2.2",
    "express": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.1",
    "nock": "^13.3.2",
    "openapi-types": "^12.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "webpack": "^5.99.9",
    "winston": "^3.13.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-sonar": "^0.2.16",
    "prettier": "^3.4.2",
    "sonarqube-scanner": "^3.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.1"
  }
}
